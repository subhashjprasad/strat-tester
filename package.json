{
  "name": "strat-tester",
  "version": "1.0.0",
  "description": "Trading strategy testing framework",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "client": "cd client && npm start",
    "client:build": "cd client && npm run build",
    "client:install": "cd client && npm install",
    "build": "npm run client:install && npm run client:build",
    "heroku-postbuild": "npm run client:install && npm run client:build",
    "render-postbuild": "npm run client:install && npm run client:build"
  },
  "engines": {
    "node": "20.x",
    "npm": "10.x"
  },
  "dependencies": {
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "multer": "^1.4.5-lts.1",
    "child_process": "^1.0.2"
  },
  "devDependencies": {
    "nodemon": "^3.0.1"
  }
}